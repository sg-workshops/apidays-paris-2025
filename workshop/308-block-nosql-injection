#!/bin/bash

. ../common

cat<<EOF

browser: 
  $proxy_root


curl:

0x10:

No protection:
curl -X POST -H 'content-type: application/json' \\
  --data-binary '{"username":"admin","password":{"\$ne":null}}' \\
  $root/api/login

With protection:
curl -X POST -H 'content-type: application/json' \\
  --data-binary '{"username":"admin","password":{"\$ne":null}}' \\
  $proxy_root/api/login



EOF

haproxy_start
